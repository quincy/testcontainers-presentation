{
  "version": 0,
  "index_patterns": [
    "person-index-*"
  ],
  "order": 0,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0"
    }
  },
  "mappings": {
    "dynamic": "false",
    "properties": {
      "firstName": {
        "type": "keyword"
      },
      "lastName": {
        "type": "keyword"
      },
      "age": {
        "type": "integer"
      }
    }
  }
}
